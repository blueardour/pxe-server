[VS_D110KMP.ndi.NTamd64]
Characteristics = 0x84
BusType         = 5
DelReg          = Kinnerth.DelReg, TaskOffload.DelReg, ALR.DelReg, 82558BDelReg.DelReg
AddReg          = Speed100.reg,ICH2.reg,Kinnereth.reg, EnablePME.reg, EnableLLPD.reg, XP_PowerSave.reg, LogLinkState.reg, VistaOS.reg
AddReg          = e100b.ndi.NT.reg, LargeSendOffload.reg, 82558B.reg
AddReg		= uninstallvista.reg
CopyFiles       = E100B_325.ndis5.CopyFiles, UninstallW2k.CopyFiles

[VS_D110KMP.ndi.NTamd64.Services]
AddService = E100B, 2, efe5b32e.Service, common325.EventLog

[VS_D110KMP.ndi.NTamd64.CoInstallers]
AddReg=CoInstaller_AddReg_VS
CopyFiles=CoInstaller_CopyFiles_VS





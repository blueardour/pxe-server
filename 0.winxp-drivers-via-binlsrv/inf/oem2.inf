[Xp32EventLog]
AddReg = Xp32AddEventLogReg

[Xp32AddEventLogReg]
HKR, , EventMessageFile, 0x00020000, "%SystemRoot%\System32\netevent.dll;%SystemRoot%\System32\drivers\k57xp32.sys"
HKR, , TypesSupported  , 0x00010001, 7


[BCM5700.Xp32AddService]
DisplayName    = %DriverDescription%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\k57xp32.sys
LoadOrderGroup = NDIS
Addreg         = bcmPnpInstallReg

[NtEventLog]
AddReg = NtAddEventLogReg

[NtAddEventLogReg]
HKR, , EventMessageFile, 0x00020000, "%SystemRoot%\System32\netevent.dll;%SystemRoot%\System32\drivers\k57w2k.sys"
HKR, , TypesSupported  , 0x00010001, 7


[BCM5700.AddService]
DisplayName    = %DriverDescription%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\k57w2k.sys
LoadOrderGroup = NDIS
Addreg         = bcmPnpInstallReg
[bcmPnpInstallReg]
HKR,,product_version,0,"14.1.0"

[DestinationDirs]

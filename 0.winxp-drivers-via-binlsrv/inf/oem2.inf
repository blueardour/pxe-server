CopyFile.XpSys32 = 12
CopyFile.NtSys = 12

[SourceDisksNames]
1 = %DISK_DESC%,,,

[SourceDisksFiles]
k57xp32.sys = 1
k57w2k.sys = 1

[CopyFile.XpSys32]
k57xp32.sys,,,1

[CopyFile.NtSys]
k57w2k.sys,,,1

[Xp32AddReg]

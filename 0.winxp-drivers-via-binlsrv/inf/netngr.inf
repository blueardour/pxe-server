BusType=5
AddReg=PNIC.reg, ngrpci.pnic.reg, ngrpci.con.reg, ngrpci.reg
;CpyFiles=NGRPCI.CopyFiles
[PNIC.ndi.Services]
AddService=ngrpci, 2, ngrpci.Service, ngrpci.EventLog
[PNIC.reg]
HKR, , AdapterCFID, 0, "000211AD"

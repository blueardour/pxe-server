
[version]
Signature="$Windows NT$"
Class=Net
ClassGUID={4d36e972-e325-11ce-bfc1-08002be10318}
Provider=%MSFT%
LayoutFile=layout.inf
DriverVer=07/01/2001,4.56.16.0
[Manufacturer]
%NETGEAR%=NETGEAR
